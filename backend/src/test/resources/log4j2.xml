<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xml>
<Configuration>
    <!-- Appenders -->
    <Appenders>
        <Console name="CONSOLE" target="SYSTEM_OUT">
            <PatternLayout pattern="%-5p: %c - %m%n" />
        </Console>
    </Appenders>
    
    <!-- Root Logger -->
    <Loggers>
        <Root level="info">
            <!-- <AppenderRef ref="CONSOLE" /> -->
        </Root>
    </Loggers>
</Configuration>